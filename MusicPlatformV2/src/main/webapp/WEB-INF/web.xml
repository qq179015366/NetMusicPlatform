<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
  	<listener-class>com.xkm.nmp.listener.LoginListener</listener-class>
  </listener>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.xkm.nmp.web.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterServlett</servlet-name>
  	<display-name>RegisterServlett</display-name>
  	<description></description>
  	<servlet-class>com.xkm.nmp.web.RegisterServlett</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UploadMusicServlet</servlet-name>
  	<display-name>UploadMusicServlet</display-name>
  	<description></description>
  	<servlet-class>com.xkm.nmp.web.UploadMusicServlet</servlet-class>
  </servlet>
  <servlet>
   <servlet-name>SearchServlet</servlet-name>
   <display-name>SearchServlet</display-name>
   <description></description>
   <servlet-class>com.xkm.nmp.web.SearchServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MusicListServlet</servlet-name>
  	<display-name>MusicListServlet</display-name>
  	<description></description>
  	<servlet-class>com.xkm.nmp.web.MusicListServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserServlet</servlet-name>
  	<display-name>UserServlet</display-name>
  	<description></description>
  	<servlet-class>com.xkm.nmp.web.UserServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>MusicServlet</servlet-name>
  	<display-name>MusicServlet</display-name>
  	<description></description>
  	<servlet-class>com.xkm.nmp.web.MusicServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterServlett</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UploadMusicServlet</servlet-name>
  	<url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
   <servlet-name>SearchServlet</servlet-name>
   <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MusicListServlet</servlet-name>
  	<url-pattern>/musiclist</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserServlet</servlet-name>
  	<url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MusicServlet</servlet-name>
  	<url-pattern>/music</url-pattern>
  </servlet-mapping>
</web-app>
